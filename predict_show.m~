function predict_show(T2_test,Y2_test,YV)
figure
%subplot(1,3,1)
ylow = YV{30}(:,1) - 1.96*sqrt(YV{30}(:,2));
yhigh = YV{30}(:,1) + 1.96*sqrt(YV{30}(:,2));
t = T2_test{30};y = [ylow  YV{30}(:,1) yhigh];
fy=cat(2,yhigh',flipdim(ylow,1)')';
fx=cat(2,t',flipdim(t,1)')';
fill(fx,fy,[0.95,0.95,1.0],'EdgeColor','w') ; hold on
[T2,I]=sort(T2_test{30})
plot(T2,Y2_test{30}(I),'r+',T2,YV{30}(:,1),'g');%ylim([-2 2]);
figure;%subplot(1,3,2)
ylow = YV{60}(:,1) - 1.96*sqrt(YV{60}(:,2));
yhigh = YV{60}(:,1) + 1.96*sqrt(YV{60}(:,2));
t = T2_test{60};y = [ylow  YV{60}(:,1) yhigh];
fy=cat(2,yhigh',flipdim(ylow,1)')';
fx=cat(2,t',flipdim(t,1)')';
fill(fx,fy,[0.95,0.95,1.0],'EdgeColor','w') ; hold on
plot(T2_test{60},Y2_test{60},'r+',T2_test{60},YV{60}(:,1),'g');%ylim([-2 2]);
figure;%subplot(1,3,3)
ylow = YV{90}(:,1) - 1.96*sqrt(YV{90}(:,2));
yhigh = YV{90}(:,1) + 1.96*sqrt(YV{90}(:,2));
t = T2_test{90};y = [ylow  YV{90}(:,1) yhigh];
fy=cat(2,yhigh',flipdim(ylow,1)')';
fx=cat(2,t',flipdim(t,1)')';
fill(fx,fy,[0.95,0.95,1.0],'EdgeColor','w') ; hold on
plot(T2_test{90},Y2_test{90},'r+',T2_test{90},YV{90}(:,1),'g');%ylim([-2 2]);
